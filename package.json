{
  "name": "genesis",
  "version": "1.0.0",
  "description": "A simple ReactJS starter kit that I've put together in order to figure out life in general",
  "author": "Tiago Pina <tiagopina@gmail.com> (http://github.com/tpina)",
  "license": "MIT",
  "keywords": [
    "babel",
    "boilerplate",
    "express",
    "hot reloading",
    "react",
    "react-hot-reloader",
    "starter",
    "webpack"
  ],
  "main": "index.js",
  "scripts": {
    "reset": "rimraf dist/* && rimraf node_modules/* && yarn",
    "deep-clean": "rimraf dist/* && rimraf node_modules/*",
    "clean": "rimraf dist/*",
    "lint": "eslint ./src",
    "start": "cross-env NODE_ENV=development babel-node ./server.js",
    "build-dev": "npm run clean && cross-env NODE_ENV=development webpack --config config/webpack.config.dev.babel.js",
    "build-prod": "npm run clean && cross-env NODE_ENV=production webpack --config config/webpack.config.prod.babel.js",
    "serve": "npm run build-dev && cross-env NODE_ENV=development babel-node server.js",
    "serve-dist": "npm run build-prod && cross-env NODE_ENV=production babel-node server.js",
    "test": "jest"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-register": "^6.16.3",
    "bootstrap-loader": "^2.0.0-beta.12",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "enzyme": "^2.5.1",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "^0.9.0",
    "font-awesome-loader": "^1.0.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "http-proxy": "^1.15.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^16.0.1",
    "node-sass": "^3.10.1",
    "open-browser-webpack-plugin": "^0.0.3",
    "postcss-loader": "^1.0.0",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.3.2",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.0",
    "webpack-merge": "^0.15.0"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "express": "^4.14.0",
    "font-awesome": "^4.6.3",
    "jquery": "^3.1.1",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.5",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
