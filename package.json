{
  "name": "genesis",
  "version": "1.0.0",
  "description": "ReactJS project with API data fetching",
  "author": "Tiago Pina <tiagopina@gmail.com> (http://github.com/tpina)",
  "license": "MIT",
  "keywords": [
    "babel",
    "boilerplate",
    "express",
    "hot reloading",
    "react",
    "react-hot-reloader",
    "starter",
    "webpack"
  ],
  "main": "index.js",
  "scripts": {
    "reset": "rimraf dist/* && rimraf node_modules/* && yarn",
    "deep-clean": "rimraf dist/* && rimraf node_modules/*",
    "clean": "rimraf dist/*",
    "lint": "eslint ./src",
    "start": "cross-env NODE_ENV=development babel-node ./server.js",
    "build-dev": "npm run clean && cross-env NODE_ENV=development webpack --config config/webpack.config.babel.js",
    "build-prod": "npm run clean && cross-env NODE_ENV=production webpack --config config/webpack.config.babel.js",
    "serve": "npm run build-dev && cross-env NODE_ENV=development babel-node server.js",
    "serve-dist": "npm run build-prod && cross-env NODE_ENV=production babel-node server.js",
    "deploy": "npm run build-prod && firebase deploy",
    "test": "jest"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "axios-mock-adapter": "^1.7.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "bootstrap-loader": "^2.0.0-beta.12",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "^0.9.0",
    "font-awesome-loader": "^1.0.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.26.0",
    "http-proxy": "^1.16.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.1.0",
    "node-sass": "^4.1.1",
    "open-browser-webpack-plugin": "^0.0.3",
    "postcss-loader": "^1.2.1",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.4.1",
    "redux-devtools": "^3.3.1",
    "redux-mock-store": "^1.2.1",
    "resolve-url-loader": "^1.6.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta",
    "webpack-dashboard": "^0.2.1",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-merge": "^2.1.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "express": "^4.14.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "moment": "^2.17.1",
    "ramda": "^0.23.0",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-promise-middleware": "^4.2.0",
    "redux-thunk": "^2.1.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
